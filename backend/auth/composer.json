{
    "name": "thomas/auth",
    "description": "Serviço de autenticação e autorização de usuário",
    "require": {
        "ramsey/uuid": "^4.7",
        "firebase/php-jwt": "^6.10",
        "guzzlehttp/guzzle": "^7.8",
        "slim/slim": "4.*",
        "slim/psr7": "^1.6",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.6",
        "php-amqplib/php-amqplib": "3.6.2"
    },
    "require-dev": {
        "pestphp/pest": "^2.34"
    },
    "autoload": {
        "psr-4": {
            "Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Thomas Moraes",
            "email": "tho.vini7@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
